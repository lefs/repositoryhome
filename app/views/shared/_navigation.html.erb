<nav>
  <%= link_to image_tag("repositoryhome.png", alt: "Repository Home"), root_path, id: "logo" %>
  <div>
    <span><%= current_user.email %></span>
    <span><%= link_to 'Settings', settings_path, id: 'settings-link', class: current_page?(settings_path) ? "active-settings" : "" %></span>
      <%= link_to 'Sign out', destroy_user_session_path, method: :delete %>
  </div>
</nav>
<%= admins_only do %>
<div id="admin-background"></div>
<ul id="admin-navigation">
  <li class="tab-label <%= "active-tab" if current_page?(root_path) or current_page?(repositories_path)%>">
    <%= link_to 'Repositories', repositories_path %>
  </li>
  <li class="tab-label <%= "active-tab" if current_page?(users_path) %>">
    <%= link_to 'Users', users_path %>
  </li>
</ul>
<% end %>
